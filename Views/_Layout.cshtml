@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Jacqui Concolino - LPGA Golfer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Jacqui Concolino LPGA Golfer">

    <link href="../Content/bootstrap.css" rel="stylesheet" />

    <style type="text/css">
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
    </style>
    <link href="../Content/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="../Content/lightbox.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Yellowtail' rel='stylesheet' type='text/css'>

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="/">Jacqui Concolino</a>
        </div>
      </div>
    </div>

    <div class="container">

      <div class="hero-unit">
          @RenderBody()
      </div>

      <div class="row">
        <div class="child-unit span4">
          @Html.Partial("Home/_Upcoming")
        </div>
        <div class="child-unit span4">
          @Html.Partial("Home/_Recent")
       </div>
        <div class="child-unit span4">
          @Html.Partial("Home/_Bio")
        </div>
      </div>

      <hr>

      <footer>
        <p>&copy; Jacqui Concolino / Photos: Getty Images</p>
      </footer>

    </div>

     <script src="../Content/Scripts/jquery-1.8.1.min.js"></script>
     <script src="../Content/Scripts/bootstrap.min.js"></script>
     <script src="../Content/Scripts/lightbox.js"></script>
     <script type="text/javascript">
         $(document).ready(function () {
             $('.carousel').carousel()
         });
     </script>
  </body>
</html>
